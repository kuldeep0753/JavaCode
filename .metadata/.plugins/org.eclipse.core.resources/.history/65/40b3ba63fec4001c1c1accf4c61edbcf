package TriangularPattern;

import java.util.Scanner;

public class TriangularPattern3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		int n=s.nextInt(),i=1;
		while(i<=n) {
			int j=1, gap=0;
			while(j<=i) {
				System.out.print(i+gap +" ");
				
				gap++;
				j++;
			}
			System.out.println();
			i++;
		}
	}

}
