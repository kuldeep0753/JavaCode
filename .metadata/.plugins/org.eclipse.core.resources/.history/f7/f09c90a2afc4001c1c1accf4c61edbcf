import java.util.Scanner;
public class SquarePattern2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		int n=s.nextInt(),i=1;
		while(i<=n) {
			int j=n;
			while(j<=n) {
				System.out.print(j+" ");
				j--;
			}
			System.out.println("");
			i++;
		}
	}

}
