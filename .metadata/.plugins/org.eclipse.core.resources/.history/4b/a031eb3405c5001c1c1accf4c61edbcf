package CharacterPatterns;

import java.util.Scanner;

public class CharacterPattern2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s=new Scanner(System.in);
		int n=s.nextInt(),i=1;
		char p='A';
		while(i<=n) {
			int j=1;
			while(j<=n) {
//				(char)('A'+j-1)
				System.out.print(p);
				j++;
			}
			System.out.println("");
			i++;
			p++;
		}
	}

}
